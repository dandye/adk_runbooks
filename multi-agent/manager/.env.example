# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# SOAR Integration (Siemplify)
SOAR_APP_KEY=your_soar_app_key_here
SOAR_URL="https://your-instance.siemplify-soar.com"

# VirusTotal
VT_APIKEY=your_virustotal_api_key_here

# Google Chronicle
CHRONICLE_REGION=us
CHRONICLE_PROJECT_ID=your_project_id_here
CHRONICLE_CUSTOMER_ID=76543210-dcba-4321-abcd-ba9876543210

# MCP Security Tool Paths
# Update these paths to match your local MCP security installation
# These should point to the directories containing server.py for each MCP tool
MCP_SIEM_DIR=/path/to/mcp_security/server/secops/secops_mcp
MCP_SOAR_DIR=/path/to/mcp_security/server/secops-soar/secops_soar_mcp
MCP_GTI_DIR=/path/to/mcp_security/server/gti/gti_mcp
MCP_ENV_FILE=/path/to/mcp_security/.env