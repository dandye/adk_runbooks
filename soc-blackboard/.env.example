# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Google Security Operations (Chronicle) Configuration
CHRONICLE_PROJECT_ID=your_gcp_project_id
CHRONICLE_CUSTOMER_ID=your_chronicle_customer_id
CHRONICLE_REGION=us

# Google Security Operations SOAR Configuration
SOAR_URL=https://your-soar-instance.example.com
SOAR_APP_KEY=your_soar_app_key

# Google Threat Intelligence Configuration
# (Uses the same GCP project authentication as Chronicle)

# Security Command Center Configuration
# (Uses the same GCP project authentication as Chronicle)

# Legacy VirusTotal Configuration (optional - GTI provides similar capabilities)
VT_APIKEY=your_virustotal_api_key

# Investigation Settings
MAX_INVESTIGATION_DURATION=3600  # Maximum investigation time in seconds
DEFAULT_CONFIDENCE_THRESHOLD=0.7  # Minimum confidence for findings
ENABLE_AUTO_CORRELATION=true  # Enable automatic correlation analysis
REPORT_FORMAT=markdown  # Report format: markdown or pdf

# Blackboard Settings
BLACKBOARD_RETENTION_DAYS=90  # How long to retain investigation data
MAX_FINDINGS_PER_AREA=1000  # Maximum findings per knowledge area
ENABLE_FINDING_DEDUPLICATION=true  # Remove duplicate findings

# Agent Settings
AGENT_TIMEOUT=300  # Individual agent timeout in seconds
ENABLE_PARALLEL_EXECUTION=true  # Run investigators in parallel
MAX_PARALLEL_AGENTS=5  # Maximum concurrent agents

# Logging Configuration
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=soc_blackboard.log
ENABLE_CONSOLE_LOGGING=true

# Optional: External Integration
# SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
# EMAIL_NOTIFICATIONS=security-team@example.com
# JIRA_URL=https://your-company.atlassian.net
# JIRA_API_TOKEN=your_jira_api_token